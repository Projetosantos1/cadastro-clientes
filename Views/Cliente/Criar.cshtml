@{
    ViewData["Title"] = "Insere Cliente";
}

<h1>INSERIR CLIENTE</h1>
<form>
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label"> Criar nome</label>
        <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
        <div id="emailHelp" class="form-text">Crie seu nome</div>
    </div>
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label"> Criar Email </label>
        <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
        <div id="emailHelp" class="form-text">Crie seu email</div>
    </div>
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label"> Criar senha</label>
        <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
        <div id="emailHelp" class="form-text">Crie sua senha</div>
    </div>

    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label"> Criar telefone</label>
        <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
        <div id="emailHelp" class="form-text">Coloque seu telefone</div>
    </div>


    <button type="submit" class="btn btn-primary">Inserir</button>
</form>